<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <dimen name="line_width_default">2dp</dimen>
    <integer name="legend_line_count_default">6</integer>
    <dimen name="start_legend_margin_default">0dp</dimen>
    <dimen name="bottom_legend_margin_default">6dp</dimen>
    <dimen name="legend_text_size_default">13sp</dimen>
    <dimen name="grid_line_width_default">1dp</dimen>

    <dimen name="abscissa_legend_margin_top_default">4dp</dimen>
    <integer name="abscissa_legend_count_default">5</integer>
    <fraction name="abscissa_legend_horizontal_margin_percent_default">0%</fraction>

    <dimen name="frame_corner_radius_default">8dp</dimen>
    <dimen name="frame_thickness_horizontal_default">2dp</dimen>
    <dimen name="frame_thickness_vertical_default">14dp</dimen>
    <fraction name="frame_min_width_percent_default">10%</fraction>
    <fraction name="frame_max_width_percent_default">100%</fraction>
    <dimen name="frame_drag_indicator_max_height_default">24dp</dimen>
    <dimen name="frame_drag_indicator_width_default">4dp</dimen>
    <dimen name="frame_drag_indicator_corner_radius_default">4dp</dimen>

    <dimen name="popup_line_width">1dp</dimen>
    <dimen name="point_inner_radius">3dp</dimen>
    <dimen name="point_outer_radius">5dp</dimen>

    <dimen name="dy_stop_tracking_touch">50dp</dimen>
    <fraction name="epsilon_x_tracking_touch">3%</fraction>
</resources>
